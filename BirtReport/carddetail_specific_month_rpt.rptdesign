            <xml-property name="queryText"><![CDATA[select period_analysis, cardcan, cardctc,txnTypeCode, BalanceBefore, txnAmount, 
 (CASE WHEN TXNTYPECODE = '210' OR TXNTYPECODE = '209' or TXNTYPECODE = '193' OR TXNTYPECODE = '196' OR TXNTYPECODE = '198' THEN txnamount ELSE 0 END
				 	) 
				  AS CASH_IN , 
           (CASE WHEN TXNTYPECODE = '210' OR TXNTYPECODE = '209' or TXNTYPECODE = '193' OR TXNTYPECODE = '196' OR TXNTYPECODE = '198' THEN 1 ELSE 0 END
				 	) 
				  AS CASH_IN_COUNT , 				  
			(
				 CASE WHEN TXNTYPECODE = '211' or  TXNTYPECODE = '212' OR  TXNTYPECODE = '194' OR TXNTYPECODE = '177' OR TXNTYPECODE = '192' OR TXNTYPECODE = '202' THEN txnamount ELSE 0 END
				 ) *-1
				  AS CASH_OUT ,
			(
				 CASE WHEN TXNTYPECODE = '211' or  TXNTYPECODE = '212' OR  TXNTYPECODE = '194' OR TXNTYPECODE = '177'OR TXNTYPECODE = '192'  OR TXNTYPECODE = '202'  THEN 1 ELSE 0 END
				 )
				  AS CASH_OUT_COUNT	,	   
				  				  

BalanceAfter
from txnJournal  (readpast)
where cardcan = ? and 
txnTypecode <> '204' and
period_analysis_month = ?  order by cardctc ]]></xml-property>
